{
    "name": "ndinhbang/response-cache",
    "description": " A laravel package to cache entire response",
    "keywords": [
        "ndinhbang",
        "response-cache"
    ],
    "homepage": "https://github.com/ndinhbang/response-cache",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dinh Bang Nguyen",
            "email": "ndinhbang111@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ndinhbang\\ResponseCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ndinhbang\\ResponseCache\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ndinhbang\\ResponseCache\\ResponseCacheServiceProvider"
            ],
            "aliases": {
                "ResponseCache": "Ndinhbang\\ResponseCache\\ResponseCacheFacade"
            }
        }
    }
}
